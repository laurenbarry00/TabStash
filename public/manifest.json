{
  "manifest_version": 2,
  "name": "TabStash",
  "author": "Lauren Barry",
  "version": "0.1",
  "description": "git stash but for Chrome tabs.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open TabStash"
  },
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  }
}
